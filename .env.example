SECRET_KEY=your-secret-key-here-change-this-in-production
ADMIN_USERNAME=admin
ADMIN_PASSWORD=admin123
AWSL_TELEGRAM_STORAGE_URL=https://assets.awsl.icu
AWSL_TELEGRAM_API_TOKEN=your-telegram-api-token
AWSL_TELEGRAM_CHAT_ID=your-telegram-chat-id

# OAuth Configuration
# GitHub OAuth (https://github.com/settings/developers)
GITHUB_CLIENT_ID=your-github-client-id
GITHUB_CLIENT_SECRET=your-github-client-secret

# Linux.do OAuth (https://connect.linux.do)
LINUXDO_CLIENT_ID=your-linuxdo-client-id
LINUXDO_CLIENT_SECRET=your-linuxdo-client-secret

# Database Configuration
# For SQLite (local development):
# DATABASE_URL=sqlite+aiosqlite:///./videos.db
# For PostgreSQL (production):
DATABASE_URL=postgresql+asyncpg://user:password@localhost:5432/awsl_video

# Debug mode (set to true to enable SQL logging)
DEBUG=false

# IP Country Blocking Configuration
# Comma-separated list of ISO 3166-1 alpha-2 country codes to block
# Example: BLOCKED_COUNTRIES=CN,RU,KP (blocks China, Russia, North Korea)
BLOCKED_COUNTRIES=CN
